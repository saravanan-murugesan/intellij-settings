<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Copy">
        <Item>
          <IdeaActionID>$Copy</IdeaActionID>
          <ShortCut>'⌘C'</ShortCut>
          <Description>Copy</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B'</ShortCut>
          <Description>Declaration</Description>
          <Count>25</Count>
        </Item>
      </Statistic>
      <Statistic Action="Paste">
        <Item>
          <IdeaActionID>$Paste</IdeaActionID>
          <ShortCut>'⌘V'</ShortCut>
          <Description>Paste</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Reformat Code">
        <Item>
          <IdeaActionID>ReformatCode</IdeaActionID>
          <ShortCut>'⌥⌘L'</ShortCut>
          <Description>Reformat Code</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="TODO">
        <Item>
          <IdeaActionID>ActivateTODOToolWindow</IdeaActionID>
          <ShortCut>'⌘6'</ShortCut>
          <Description>TODO</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'⌥F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>16</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Case">
        <Item>
          <IdeaActionID>EditorToggleCase</IdeaActionID>
          <ShortCut>'⇧⌘U'</ShortCut>
          <Description>Toggle Case</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Version Control">
        <Item>
          <IdeaActionID>ActivateVersionControlToolWindow</IdeaActionID>
          <ShortCut>'⌘9' or '⇧⌘9'</ShortCut>
          <Description>Version Control</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>